   "source": [
    "def seq(start, stop, step):\n",
    "    \"\"\"\n",
    "    수열\n",
    "    입력 : start(시작 값), stop(끝 값), step(한 스텝당 증가)\n",
    "    출력 : res(리스트)\n",
    "    \"\"\"\n",
    "    res = []\n",
    "    current = start\n",
    "    while current < stop:\n",
    "        res.append(current)\n",
    "        current += step\n",
    "    return res\n",
    "\n",
    "def seq(start, stop, step):\n",
    "    \"\"\"\n",
    "    수열\n",
    "    입력 : start(시작 값), stop(끝 값), step(한 스텝당 증가)\n",
    "    출력 : res(리스트)\n",
    "    \"\"\"\n",
    "    res = []\n",
    "    current = start\n",
    "    while current < stop:\n",
    "        res.append(current)\n",
    "        current += step\n",
    "    return res\n",
    "\n",
    "def gamma(alpha):\n",
    "    \"\"\"\n",
    "    감마\n",
    "    입력 : alpha\n",
    "    출력 : res(실수)\n",
    "    \"\"\"\n",
    "\n",
    "    a = 0\n",
    "    b = 100\n",
    "    x_list = seq(0.0001, 100, 0.001)\n",
    "    gamma_list = []\n",
    "    for x in x_list:\n",
    "        y = gamma_function(alpha ,x)\n",
    "        gamma_list.append(y)\n",
    "    res = ((b-a)/len(x_list))*sum(gamma_list)\n",
    "    return res"
   ]
  },
