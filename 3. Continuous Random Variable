   "source": [
    "e = 2.7182818284\n",
    "\n",
    "beta = 2\n",
    "x_list = seq(0, beta*10, 0.01)\n",
    "p_list = []\n",
    "\n",
    "for x in x_list:\n",
    "    prob = (1/beta)*(e**((-1/beta)*x))\n",
    "    p_list.append(prob)\n",
    "\n",
    "plt.plot(x_list, p_list)\n",
    "plt.show()"
   ]
  },
