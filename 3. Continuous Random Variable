    "def gamma(alpha):\n",
    "    \"\"\"\n",
    "    감마\n",
    "    입력 : alpha\n",
    "    출력 : res(실수)\n",
    "    \"\"\"\n",
    "\n",
    "    a = 0\n",
    "    b = 100\n",
    "    x_list = seq(0.0001, 100, 0.001)\n",
    "    gamma_list = []\n",
    "    for x in x_list:\n",
    "        y = gamma_function(alpha ,x)\n",
    "        gamma_list.append(y)\n",
    "    res = ((b-a)/len(x_list))*sum(gamma_list)\n",
    "    return res"
   ]
  },
