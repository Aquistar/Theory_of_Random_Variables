    "def gamma_function(alpha,x):\n",
    "    \"\"\"\n",
    "    감마 함수\n",
    "    입력 : alpha, x\n",
    "    출력 : res(실수)\n",
    "    \"\"\"\n",
    "\n",
    "    e = 2.7182818284\n",
    "    res = (x**(alpha-1))*(e**(-x))\n",
    "    return res"
   ]
  },
